location ^~/R/Web/{
    proxy_pass http://${RHOST}:${RPORT}$request_uri;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
}

location ^~/R/WebRWebSite/WebR/ {  
    alias  /WebR2/;
    error_page 405 =200 $uri;
}
location /R/Ws {
    proxy_pass http://${RHOST}:${RWS}/R/Ws;
    proxy_http_version 1.1;
    proxy_connect_timeout 4s;
    proxy_read_timeout 60s;
    proxy_send_timeout 12s;
    proxy_set_header Upgrade $http_upgrade;
    proxy_set_header Connection "upgrade";
}

location ^~/Setting3/API {
    proxy_pass http://${RHOST}:${RPORT}$request_uri;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
}

location ^~/Setting3/ {
    alias  /Setting3/;
    error_page 405 =200 $uri;
}

location ^~/FTPUpload/API {
    proxy_pass http://${RHOST}:${RPORT}$request_uri;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
}

location ^~/FTPUpload {
    index Default.html;
    autoindex on;
    alias  /FTPUploadService/;
    error_page 405 =200 $uri;
}

location ^~/files {
    proxy_pass http://${RHOST}:50723/;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
}
location ^~/static/ {
    proxy_pass http://${RHOST}:50723$request_uri;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
}
location ^~/api/ {
    proxy_pass http://${RHOST}:50723$request_uri;
    proxy_set_header X-Forwarded-For $proxy_add_x_forwarded_for;
}